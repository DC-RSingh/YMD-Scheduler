{
  "name": "ymd-scheduler",
  "version": "1.0.0",
  "description": "A scheduling application for a small music store.",
  "main": ".webpack/main",
  "private": "true",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron-forge start -- --no-sandbox",
    "start": "cross-env NODE_ENV=production electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .ts",
    "postinstall": "npx prisma db push",
    "build-windows": "electron-builder -w",
    "build-mac": "electron-builder -m",
    "build-linux": "electron-builder -l"
  },
  "build": {
    "appId": "com.durhamcollege.ymd-scheduler",
    "productName": "Your Music Depot Scheduler",
    "files": [
      "public/icon.*"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "license": "LICENSE"
    },
    "directories": {
      "buildResources": "src/assets/*"
    },
    "extraResources": [
      "prisma/**/*"
    ]
  },
  "keywords": [],
  "author": {
    "name": "DC-RSingh",
    "email": "raje.singh@dcmail.ca"
  },
  "contributors": [
    {
      "name": "Brett Tindall",
      "email": "brett.tindall@dcmail.ca",
      "url": "https://github.com/Sub4Sub"
    },
    {
      "name": "Taylor Dixon",
      "email": "taylor.dixon@dcmail.ca",
      "url": "https://github.com/twdixon"
    },
    {
      "name": "MJ Galbraith",
      "email": "murray.galbraith@dcmail.ca",
      "url": "https://github.com/MJGalbraith31"
    },
    {
      "name": "S M Hasibur Rahman",
      "email": "smhasibur.rahman@dcmail.ca",
      "url": "https://github.com/Hasib001"
    }
  ],
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ymd_scheduler"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devServer": {},
            "mainConfig": "./config/webpack.main.config.js",
            "renderer": {
              "config": "./config/webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./public/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ],
              "node-integration": true
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-table": "^7.7.8",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "concurrently": "^6.4.0",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "electron": "15.2.0",
    "electron-builder": "^22.14.5",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.1",
    "node-loader": "^2.0.0",
    "prisma": "^3.6.0",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.2",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@devexpress/dx-react-core": "^2.7.6",
    "@devexpress/dx-react-scheduler": "^2.7.6",
    "@devexpress/dx-react-scheduler-material-ui": "^2.7.6",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/lab": "^5.0.0-alpha.58",
    "@mui/material": "^5.2.2",
    "@mui/styles": "^5.2.2",
    "@prisma/client": "^3.6.0",
    "@sweetalert2/themes": "^5.0.8",
    "@syncfusion/ej2-react-schedule": "^19.3.55",
    "@types/reactour": "^1.18.2",
    "clsx": "^1.1.1",
    "copy-webpack-plugin": "^10.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "formik": "^2.2.9",
    "localforage": "^1.10.0",
    "mobx": "^6.3.8",
    "mobx-persist": "^0.4.1",
    "mobx-react-lite": "^3.2.2",
    "mobx-utils": "^6.0.4",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.1.2",
    "react-table": "^7.7.0",
    "reactour": "^1.18.6",
    "ts-node": "^10.4.0",
    "yup": "^0.32.11"
  }
}
